<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#"
     xml:base="http://www.semanticweb.org/lokiy/ontologies/makeup.owl"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/lokiy/ontologies/makeup.owl">
        <rdfs:label>Makeup</rdfs:label>
        <license>This work is licensed under a Creative Commons Attribution-NonCommercial 4.0 International License. 

&lt;a rel=&quot;license&quot; href=&quot;http://creativecommons.org/licenses/by-nc/4.0/&quot;&gt;&lt;img alt=&quot;Creative Commons Licence&quot; style=&quot;border-width:0&quot; src=&quot;https://i.creativecommons.org/l/by-nc/4.0/88x31.png&quot; /&gt;&lt;/a&gt;&lt;br /&gt;This work is licensed under a &lt;a rel=&quot;license&quot; href=&quot;http://creativecommons.org/licenses/by-nc/4.0/&quot;&gt;Creative Commons Attribution-NonCommercial 4.0 International License&lt;/a&gt;.</license>
        <skos:definition>Sephora makeup product list, retrevied from https://www.sephora.com/ca/en/ Noted the content may have been changed for the purpose of this project.</skos:definition>
        <rdfs:comment xml:lang="en">A makeup ontology that describes makeup products</rdfs:comment>
        <Contributor>Lokiy Wang</Contributor>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Contributor -->

    <owl:AnnotationProperty rdf:about="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Contributor"/>
    


    <!-- http://www.semanticweb.org/lokiy/ontologies/makeup.owl#license -->

    <owl:AnnotationProperty rdf:about="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#license"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#altLabel -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#altLabel"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#definition -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#definition"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/lokiy/ontologies/makeup.owl#hasCountryOfOrigins -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#hasCountryOfOrigins">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Product"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Country"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/lokiy/ontologies/makeup.owl#hasSameFuncationwith -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#hasSameFuncationwith">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Makeup_Tools"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Makeup_Tools"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/lokiy/ontologies/makeup.owl#hasSkinType -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#hasSkinType">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Cosmetics"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Skin_Type"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/lokiy/ontologies/makeup.owl#isInFormularOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#isInFormularOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#FoundationMakeup_Product"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Formular"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/lokiy/ontologies/makeup.owl#isUsedFor -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#isUsedFor">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#isUsedWith"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Makeup_Tools"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Cosmetics"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/lokiy/ontologies/makeup.owl#isUsedWith -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#isUsedWith">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Cosmetics"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Makeup_Tools"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Blender_Sponges -->

    <owl:Class rdf:about="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Blender_Sponges">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Sponges"/>
        <skos:definition xml:lang="en">Sponges specificly used for blending purpose
sources: https://www.sephora.com/ca/en/shop/makeup-cosmetics</skos:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Brush -->

    <owl:Class rdf:about="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Brush">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Makeup_Tools"/>
        <skos:definition xml:lang="en">Any brush used for applying cosmetics

sources: https://www.sephora.com/ca/en/shop/makeup-cosmetics</skos:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Combination -->

    <owl:Class rdf:about="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Combination">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Skin_Type"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lokiy/ontologies/makeup.owl#ConcealerMakeup_Product -->

    <owl:Class rdf:about="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#ConcealerMakeup_Product">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#EyeMakeup_Product"/>
        <skos:definition>Concealer that brightens the under-eyes

sources: https://www.sephora.com/ca/en/shop/makeup-cosmetics</skos:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Cosmetics -->

    <owl:Class rdf:about="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Cosmetics">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Product"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Makeup_Tools"/>
        <rdfs:comment xml:lang="en">Replacements parts of the tools comes with cosmetics will be listed in Makeup Tools sepereately</rdfs:comment>
        <skos:altLabel xml:lang="en">Cosmetics products, not including application tools</skos:altLabel>
        <skos:definition xml:lang="en">cosmetics products, not including tools. Cosmetics comes with tools such as facial pressed powder may comes with sponge inside still counts as cosmetics.

sources: https://www.sephora.com/ca/en/shop/makeup-cosmetics</skos:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Country -->

    <owl:Class rdf:about="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Country">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Sephora"/>
        <skos:altLabel>Country of Origins</skos:altLabel>
        <skos:definition>Country of origins which the product was produced, the selection can only be one country.</skos:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Cream -->

    <owl:Class rdf:about="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Cream">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Formular"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#hasSkinType"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Combination"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#hasSkinType"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Dry"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#hasSkinType"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Normal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Curling_Iron -->

    <owl:Class rdf:about="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Curling_Iron">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Hair_Tools"/>
        <skos:definition xml:lang="en">Hair tools used for curling style
sources: https://www.sephora.com/ca/en/shop/makeup-cosmetics</skos:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Dry -->

    <owl:Class rdf:about="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Dry">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Skin_Type"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lokiy/ontologies/makeup.owl#EyeMakeup_Product -->

    <owl:Class rdf:about="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#EyeMakeup_Product">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Cosmetics"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#isUsedWith"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Eye_Brush"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#hasCountryOfOrigins"/>
                <owl:hasValue rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#UK"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition xml:lang="en">Cosmetic products used for eye area</skos:definition>
        <skos:definition>Makeup products for eye area
sources: https://www.sephora.com/ca/en/shop/makeup-cosmetics</skos:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Eye_Brush -->

    <owl:Class rdf:about="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Eye_Brush">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Brush"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#isUsedFor"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#EyeMakeup_Product"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition>brush for eye application
: https://www.sephora.com/ca/en/shop/makeup-cosmetics</skos:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Eye_PrimerMakeup_Product -->

    <owl:Class rdf:about="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Eye_PrimerMakeup_Product">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#EyeMakeup_Product"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Eye_SetsMakeup_Product -->

    <owl:Class rdf:about="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Eye_SetsMakeup_Product">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#EyeMakeup_Product"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lokiy/ontologies/makeup.owl#EyebrowMakeup_Product -->

    <owl:Class rdf:about="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#EyebrowMakeup_Product">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#EyeMakeup_Product"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Eyelash_Curler -->

    <owl:Class rdf:about="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Eyelash_Curler">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Facial_Accessories"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#isUsedFor"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#False_EyelashesMakeup_Product"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition xml:lang="en">Any eyelash curler, regardless of the matrials such as eletronic or plastics.
sources: https://www.sephora.com/ca/en/shop/makeup-cosmetics</skos:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lokiy/ontologies/makeup.owl#EyelinerMakeup_Product -->

    <owl:Class rdf:about="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#EyelinerMakeup_Product">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#EyeMakeup_Product"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lokiy/ontologies/makeup.owl#FaceMakeup_Product -->

    <owl:Class rdf:about="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#FaceMakeup_Product">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Cosmetics"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#isUsedWith"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Brush"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#isUsedWith"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Sponges"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#hasCountryOfOrigins"/>
                <owl:hasValue rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Canada"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:altLabel xml:lang="en">Not including products for cheek, eye, and lip.</skos:altLabel>
        <skos:definition xml:lang="en">Cosmetic products used for face area

sources: https://www.sephora.com/ca/en/shop/makeup-cosmetics</skos:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Face_Brush -->

    <owl:Class rdf:about="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Face_Brush">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Brush"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#hasSameFuncationwith"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Sponges"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Brush"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#isUsedFor"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#FaceMakeup_Product"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition>brush for face application
sources: https://www.sephora.com/ca/en/shop/makeup-cosmetics</skos:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Facial_Accessories -->

    <owl:Class rdf:about="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Facial_Accessories">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Makeup_Tools"/>
        <skos:definition xml:lang="en">Any other tools used for applying cosmetics for face only that is not belonging to brush, sponges
sources: https://www.sephora.com/ca/en/shop/makeup-cosmetics</skos:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lokiy/ontologies/makeup.owl#False_EyelashesMakeup_Product -->

    <owl:Class rdf:about="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#False_EyelashesMakeup_Product">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#EyeMakeup_Product"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Flawless_Finish_Sponges -->

    <owl:Class rdf:about="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Flawless_Finish_Sponges">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Sponges"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#hasSkinType"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Combination"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#hasSkinType"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Oily"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition xml:lang="en">Sponges specificly used for finishing purpose
sources: https://www.sephora.com/ca/en/shop/makeup-cosmetics</skos:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Formular -->

    <owl:Class rdf:about="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Formular">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#FoundationMakeup_Product"/>
        <skos:definition>foundation listed by formular

sources: https://www.sephora.com/ca/en/shop/makeup-cosmetics</skos:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lokiy/ontologies/makeup.owl#FoundationMakeup_Product -->

    <owl:Class rdf:about="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#FoundationMakeup_Product">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#FaceMakeup_Product"/>
        <skos:definition>face foundation, includes all formular
sources: https://www.sephora.com/ca/en/shop/makeup-cosmetics</skos:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Full_Coverage_Sponges -->

    <owl:Class rdf:about="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Full_Coverage_Sponges">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Sponges"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#hasSkinType"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Combination"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#hasSkinType"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/lokiy/ontologies/makeup.owl#Dry"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
